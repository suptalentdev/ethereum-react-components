{
  "plugins": [
    ["@babel/plugin-transform-runtime",      {
      "corejs": false,
      "helpers": true,
      "regenerator": true,
      "useESModules": false
    }],
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "production": {
      "presets": ["@babel/preset-env", "@babel/preset-react"]
    },
    "development": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": ["styled-jsx/babel"]
    },
    "test": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": ["styled-jsx/babel"]
    }
  }
}